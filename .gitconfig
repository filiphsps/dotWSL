[apply]
	whitespace = fix
[core]
	attributesfile = ~/.gitattributes
	whitespace = space-before-tab,-indent-with-non-tab,trailing-space
	trustctime = false
	editor = vim
	autocrlf = input
	eol = lf
[init]
	defaultBranch = master
[diff]
	renames = copies
[help]
	autocorrect = 1
[color]
	ui = auto
[user]
	name = Filiph Siitam Sandstr√∂m
	email = filiph@nordcom.io
[commit]
#	gpgsign = true
[pull]
	rebase = true
[push]
	autoSetupRemote = true
